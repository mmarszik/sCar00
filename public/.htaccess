RewriteEngine On

RewriteRule ^.*css/([a-zA-Z0-9_]{3,20})\.css$            css/$1.css   [L]
RewriteRule ^.*js/([a-zA-Z0-9_\.\-]{3,20})\.js$          js/$1.js     [L]
RewriteRule ^.*imgs/([a-zA-Z0-9_]{3,20})\.jpg$           imgs/$1.jpg  [L]
RewriteRule ^.*imgs/([a-zA-Z0-9_]{3,20})\.jpeg$          imgs/$1.jpeg [L]
RewriteRule ^.*imgs/([a-zA-Z0-9_]{3,20})\.png$           imgs/$1.png  [L]
RewriteRule ^.*imgs/([a-zA-Z0-9_]{3,20})\.gif$           imgs/$1.gif  [L]

RewriteRule ^.*$                                         index.php
